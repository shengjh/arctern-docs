msgid "plot"
msgstr ""

#~ msgid ""
#~ "Plot a collection of geometries to "
#~ "`ax`. Parameters 'linewidth', 'linestyle', "
#~ "'edgecolor', 'facecolor', 'color', 'marker', "
#~ "'markersize' are used to describe the"
#~ " style of plotted figure."
#~ msgstr ""

#~ msgid ""
#~ "For geometry types `Polygon` and "
#~ "`MultiPolygon`, only 'linewidth', 'linestyle', "
#~ "'edgecolor', 'facecolor' are effective."
#~ msgstr ""

#~ msgid ""
#~ "For geometry types `Linestring` and "
#~ "`MultiLinestring`, only 'color', 'linewidth', "
#~ "'linestyle' are effective."
#~ msgstr ""

#~ msgid ""
#~ "For geometry types `Point` and "
#~ "`MultiPoint`, only 'color', 'marker', "
#~ "'markersize' are effective."
#~ msgstr ""

#~ msgid "参数"
#~ msgstr ""

#~ msgid "The axes where geometries will be plotted."
#~ msgstr ""

#~ msgid "sequence of geometries."
#~ msgstr ""

#~ msgid "The width of line, the default value is 1.0."
#~ msgstr ""

#~ msgid "The style of the line， the default value is '-'."
#~ msgstr ""

#~ msgid "The edge color of the geometry, the default value is 'black'."
#~ msgstr ""

#~ msgid "The color of the face of the geometry, the default value is 'C0'."
#~ msgstr ""

#~ msgid "The color of the geometry, the default value is 'C0'."
#~ msgstr ""

#~ msgid "The shape of point, the default value is 'o'."
#~ msgstr ""

#~ msgid "The size of points, the default value is 6.0."
#~ msgstr ""

#~ msgid "example"
#~ msgstr ""

